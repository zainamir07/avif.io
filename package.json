{
  "name": "avif.io",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "npx cross-env ANALYZE=true next build",
    "convert": "cd utils && node sharp && cd ../",
    "lint": "rustywind . --write && prettier --write . && next lint --fix --dir pages --dir components",
    "wasm": "cd conversion && npx cross-env NODE_ENV=production webpack",
    "release": "npm run build && next export && cd conversion && npx cross-env NODE_ENV=production webpack && cd ../ && next-sitemap --config next-sitemap.config.js && cd utils && node rss && cd ../"
  },
