{
  "name": "avif.io",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config scripts/sitemap.js && node --experimental-json-modules ./scripts/rss.mjs",
    "start": "next start",
    "analyze": "npx cross-env ANALYZE=true next build",
    "lint": "rustywind . --write && prettier --write . && next lint --fix --dir pages --dir components --dir lib",
    "wasm": "cd conversion && npx cross-env NODE_ENV=production webpack",
    "release": "npm run build && next export && firebase deploy"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "atropos": "^1.0.2",
    "contentlayer": "^0.2.5",
    "firebase": "^9.8.1",
    "jszip": "^3.9.1",
    "lodash": "^4.17.21",
    "next": "^12.1.6",
    "next-contentlayer": "^0.2.5",
    "preact": "^10.7.2",
    "pretty-bytes": "^6.0.0",
    "react": "^18.1.0",
    "react-compare-image": "^3.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-schemaorg": "^2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "schema-dts": "^1.1.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@types/sharp": "^0.30.2",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "next-sitemap": "^2.5.20",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rss": "^1.2.2",
    "rustywind": "^0.15.0",
    "sharp": "^0.30.4",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.2"
  }
}
